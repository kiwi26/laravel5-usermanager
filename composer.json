{
    "name": "kiwi/laravel5-usermanager",
    "description": "This package helps you to start a project with all routes, controllers and views for users management (registration, profile, password edit/reset, login, logout)",
    "keywords": [
        "kiwi",
        "user manager"
    ],
    "homepage": "https://github.com/kiwi26/laravel5-usermanager",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiwi",
            "email": "contact@kevin-anidjar.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "laravel/framework": "5.1.*",
    	"laravelcollective/html" : "5.1.*",
        "anhskohbo/no-captcha": "2.*",
	    "guzzlehttp/guzzle": "~5.3|~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Kiwi\\UserManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kiwi\\UserManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
